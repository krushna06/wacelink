{
  "name": "wacelink",
  "version": "1.0.0",
  "description": "Another lavalink wrapper but focus on stability and rich features",
  "scripts": {
    "build": "npx tsup --config tsup-config.json",
    "prettier": "npx prettier -w ./src",
    "build:doc": "typedoc --options ./typedoc.json --sort kind",
    "build:full": "npm run prettier && npm run build:eslint && npm run build && npm run build:doc",
    "build:eslint": "npx eslint ./src --fix"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "bot",
    "music",
    "lavalink.js",
    "lavalink-api",
    "discord.js",
    "eris.js",
    "oceanic.js",
    "lavalink",
    "wrapper",
    "lavalink-wrapper",
    "wacelink"
  ],
  "author": "n0step",
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.10.0",
    "@types/node": "^22.5.4",
    "@types/node-fetch": "^2.6.11",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "discord.js": "^14.14.1",
    "dokdo": "^1.0.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typedoc": "^0.26.7",
    "typescript": "^5.6.2"
  }
}
